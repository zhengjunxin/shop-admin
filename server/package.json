{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "log4js": "^2.3.3",
    "multer": "^1.3.0",
    "npm-run-all": "^4.0.2"
  },
  "scripts": {
    "start-db": "mongod --dbpath=./data",
    "start-server": "nodemon src/index.js --exec babel-node",
    "start": "npm-run-all -p start-db start-server",
    "build": "babel src -d build",
    "serve": "node build/index.js"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "mongoose": "^4.11.4",
    "nodemon": "^1.11.0"
  }
}
